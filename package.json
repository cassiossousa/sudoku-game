{
  "name": "sudoku-game",
  "version": "0.0.1",
  "description": "Sudoku Game using Vanilla JavaScript",
  "homepage": ".",
  "scripts": {
    "build": "parcel build src/index.html --dist-dir dist --public-url ./",
    "deploy": "gh-pages -d dist",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.js",
    "lint": "eslint \"src/**/*.js\"",
    "lint-staged": "lint-staged",
    "parcel": "parcel",
    "prepare": "husky",
    "preinstall": "npx npm-force-resolutions",
    "prettier": "prettier --write \"src/**/*.js\"",
    "postinstall": "husky",
    "start": "parcel src/index.html",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.less": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.12.5"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@commitlint/config-conventional": "19.6.0",
    "@parcel/transformer-less": "2.13.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "commitlint": "19.6.1",
    "eslint": "9.17.0",
    "eslint-plugin-jest": "24.1.3",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.3.0",
    "parcel": "2.13.3",
    "prettier": "3.4.2"
  },
  "resolutions": {
    "node-forge": "0.10.0",
    "ini": "1.3.8"
  },
  "keywords": [
    "sudoku",
    "game",
    "javascript",
    "browser"
  ],
  "author": "CÃ¡ssio Sousa",
  "license": "MIT"
}
