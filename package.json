{
  "name": "sudoku-game",
  "version": "0.0.1",
  "description": "Sudoku Game using Vanilla JavaScript",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -f dist/* && parcel build --no-source-maps index.html",
    "coverage": "jest --coverage --collectCoverageFrom=src/**/*.js",
    "lint": "eslint \"src/**/*.js\"",
    "preinstall": "npx npm-force-resolutions",
    "prettier": "prettier --write \"src/**/*.js\"",
    "start": "parcel index.html",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/runtime": "7.12.5"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@commitlint/config-conventional": "19.6.0",
    "babel-eslint": "10.1.0",
    "commitlint": "19.6.1",
    "eslint": "7.18.0",
    "eslint-plugin-jest": "24.1.3",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "parcel": "2.13.3",
    "prettier": "3.4.2"
  },
  "resolutions": {
    "node-forge": "0.10.0",
    "ini": "1.3.8"
  },
  "keywords": [
    "sudoku",
    "game",
    "javascript",
    "browser"
  ],
